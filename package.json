{
    "name": "next-shynet",
    "version": "2.0.0",
    "description": "A library for integrating Shynet into your next.js project",
    "repository": "git@github.com:alt-web/next-shynet.git",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs"
    },
    "keywords": [
        "shynet"
    ],
    "author": "Ivan Reshetnikov <ordinarydev@protonmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "next": "^13.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.9.3"
    },
    "peerDependencies": {
        "next": "^13.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
